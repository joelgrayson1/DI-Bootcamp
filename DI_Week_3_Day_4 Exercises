      "cell_type": "code",
      "source": [
        "#Exercise 6\n",
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "df['Panic_Attack_Numeric'] = df['Do you have Panic Attacks?'].map({'Yes': 1, 'No': 0})\n",
        "\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.scatterplot(data=df, x='Age', y='Panic_Attack_Numeric')\n",
        "\n",
        "plt.title('Relationship Between Age and Panic Attacks', fontsize=16)\n",
        "plt.xlabel('Age', fontsize=12)\n",
        "plt.ylabel('Panic Attack Occurrence (0=No, 1=Yes)', fontsize=12)\n",
        "\n",
        "plt.scatter(df['Age'], df['Panic_Attack_Numeric'], alpha=0.7, s=50)\n",
        "\n",
        "plt.show()"
